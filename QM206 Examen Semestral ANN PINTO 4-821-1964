        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "import seaborn as sns"
      ],
      "metadata": {
        "id": "gDsM1e-5wJcf"
      },
      "execution_count": 34,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def generar_posiciones(N):\n",
        "    return np.random.uniform(-1, 1, N)"
      ],
      "metadata": {
        "id": "QiRIJlC-yIeG"
      },
      "execution_count": 35,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def simular_difusion(posiciones, iteraciones, coeficiente_difusion):\n",
        "    for _ in range(iteraciones):\n",
        "        cambios_dx = np.random.uniform(-1, 1, len(posiciones))\n",
        "        posiciones += cambios_dx * np.sqrt(2 * coeficiente_difusion)\n",
        "    return posiciones"
      ],
      "metadata": {
        "id": "8OS3F8rWyNxr"
      },
      "execution_count": 36,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Parámetros\n",
        "N = 10000  # Número de partículas\n",
        "iteraciones = 1000  # Número de iteraciones del ciclo for\n",
        "coeficiente_difusion = 0.03  # Coeficiente de difusión"
      ],
      "metadata": {
        "id": "FpT8yqibyREE"
      },
      "execution_count": 42,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Posiciones iniciales\n",
        "posiciones_iniciales = generar_posiciones(N)"
      ],
      "metadata": {
        "id": "-2omWUVmybdp"
      },
      "execution_count": 38,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Posiciones finales\n",
        "posiciones_finales = simular_difusion(posiciones_iniciales.copy(), iteraciones, coeficiente_difusion)"
      ],
      "metadata": {
        "id": "qqcY8WmQyoyd"
      },
      "execution_count": 40,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Grafica histograma\n",
        "plt.hist(posiciones_finales, bins=50, density=True, alpha=0.7, color='blue', edgecolor='black')\n",
        "plt.title('Simulación de Difusión')\n",
        "plt.xlabel('Posición Final')\n",
        "plt.ylabel('Densidad de Probabilidad')\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 480
        },
        "id": "igdIU0G7y0Ub",
