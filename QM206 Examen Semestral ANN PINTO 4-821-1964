      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns"
      ],
      "metadata": {
        "id": "XmR99blgWKuy"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def generar_cadena_adn(longitud):\n",
        "    \"\"\"\n",
        "    Cadena de ADN aleatoria de la longitud especificada.\n",
        "    \"\"\"\n",
        "    nucleotidos = ['G', 'C', 'A', 'T']\n",
        "    cadena_adn = ''.join(np.random.choice(nucleotidos, size=longitud))\n",
        "    return cadena_adn"
      ],
      "metadata": {
        "id": "l9VGSsHqWxEb"
      },
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def contar_ocurrencias_secuencia(cadena_adn, secuencia):\n",
        "    \"\"\"\n",
        "    Ocurrencias de una secuencia específica en una cadena de ADN.\n",
        "    \"\"\"\n",
        "    return cadena_adn.count(secuencia)"
      ],
      "metadata": {
        "id": "xw3CbM9YW1VN"
      },
      "execution_count": 14,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def simular_experimento(num_repeticiones, longitud_cadena, secuencia):\n",
        "    \"\"\"\n",
        "    Generación de cadenas de ADN y cuenta de ocurrencias múltiples veces.\n",
        "    Lista con los resultados de las ocurrencias.\n",
        "    \"\"\"\n",
        "    resultados = []\n",
        "    for _ in range(num_repeticiones):\n",
        "        cadena_adn = generar_cadena_adn(longitud_cadena)\n",
        "        ocurrencias = contar_ocurrencias_secuencia(cadena_adn, secuencia)\n",
        "        resultados.append(ocurrencias)\n",
        "    return resultados\n"
      ],
      "metadata": {
        "id": "l2hF7xOFW4nk"
      },
      "execution_count": 17,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Parámetros\n",
        "num_repeticiones = 10000\n",
        "longitud_cadena = 1000\n",
        "secuencia = \"GCAT\""
      ],
      "metadata": {
        "id": "JhV8_mDLW82s"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Simulación del experimento\n",
        "resultados_experimento = simular_experimento(num_repeticiones, longitud_cadena, secuencia)"
      ],
      "metadata": {
        "id": "McYxghVUXBD8"
      },
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Grafica histograma\n",
        "plt.figure(figsize=(10, 6))\n",
        "sns.histplot(resultados_experimento, bins=np.arange(0, 10, 1), kde=False, color='skyblue', edgecolor='black')\n",
        "plt.title(f'Histograma de ocurrencias de la secuencia \"{secuencia}\" en cadenas de ADN')\n",
        "plt.xlabel('Número de ocurrencias')\n",
        "plt.ylabel('Frecuencia')\n",
        "plt.show()"
