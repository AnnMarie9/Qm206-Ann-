      "cell_type": "markdown",
      "source": [
        "#Problema 3\n"
      ],
      "metadata": {
        "id": "ewIWJ2gKvja8"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import pandas as pd"
      ],
      "metadata": {
        "id": "rsk7xChAjc2o"
      },
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Parámetros iniciales\n",
        "A_0 = 0.5  # M\n",
        "B_0 = 1.0  # M\n",
        "C_0 = 0.8  # M"
      ],
      "metadata": {
        "id": "JYiuTAYJkMaj"
      },
      "execution_count": 19,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Constantes de velocidad\n",
        "k1 = 0.06\n",
        "k2 = 0.09\n",
        "k3 = 0.03"
      ],
      "metadata": {
        "id": "Ayss6mydkPhJ"
      },
      "execution_count": 64,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Número de pasos de tiempo y paso de tiempo\n",
        "num_steps = 100\n",
        "dt = 0.1"
      ],
      "metadata": {
        "id": "C1TR6bIwkad3"
      },
      "execution_count": 25,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Lista de datos\n",
        "time_points = [0]\n",
        "concentration_A = [A_0]\n",
        "concentration_B = [B_0]\n",
        "concentration_C = [C_0]"
      ],
      "metadata": {
        "id": "t1U4F7jUkxfQ"
      },
      "execution_count": 65,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Simulación del mecanismo cinético\n",
        "for step in range(1, num_steps + 1):\n",
        "    # Cálculo de cambios en las concentraciones\n",
        "    dA = -k1 * concentration_A[-1]\n",
        "    dB = k1 * concentration_A[-1] - k2 * concentration_B[-1]\n",
        "    dC = k2 * concentration_B[-1] - k3 * concentration_C[-1]\n",
        "    #  Concentraciones\n",
        "    concentration_A.append(concentration_A[-1] + dA * dt)\n",
        "    concentration_B.append(concentration_B[-1] + dB * dt)\n",
        "    concentration_C.append(concentration_C[-1] + dC * dt)\n",
        "    #  Tiempo\n",
        "    time_points.append(step * dt)"
      ],
      "metadata": {
        "id": "Dj_l9E2kk2hf"
      },
      "execution_count": 66,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Convertir listas\n",
        "time_points = np.array(time_points)\n",
        "concentration_A = np.array(concentration_A)\n",
        "concentration_B = np.array(concentration_B)\n",
        "concentration_C = np.array(concentration_C)"
      ],
      "metadata": {
        "id": "ednk46GtlSPx"
      },
      "execution_count": 67,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Visualización de resultados\n",
        "data = {'Tiempo': time_points, 'A': concentration_A, 'B': concentration_B, 'C': concentration_C}\n",
        "df = pd.DataFrame(data)"
      ],
      "metadata": {
        "id": "1CZij6GelXXZ"
      },
      "execution_count": 68,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "sns.set(style=\"darkgrid\")\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.title('Evolución de las concentraciones en el tiempo')\n",
        "sns.lineplot(x='Tiempo', y='A', data=df, label='A')\n",
        "sns.lineplot(x='Tiempo', y='B', data=df, label='B')\n",
        "sns.lineplot(x='Tiempo', y='C', data=df, label='C')\n",
        "plt.xlabel('Tiempo')\n",
        "plt.ylabel('Concentración (M)')\n",
        "plt.legend()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 573
        },
        "id": "6bEWYtH6lblT",
        "outputId": "8d4fa710-c124-4248-c511-350d0b9c88e4"
      },
      "execution_count": 69,
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 1000x600 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
